{
  "actions": [
    {
      "id": "climate_now",
      "title": {
        "en": "Climate Now"
      },
      "$filter": "capabilities=climate_now_capability"
    },
    {
      "id": "lock_vehicle",
      "title": {
        "en": "Lock"
      },
      "$filter": "capabilities=only_lock_unlock_flow_capability"
    },
    {
      "id": "unlock_vehicle",
      "title": {
        "en": "Unlock"
      },
      "$filter": "capabilities=only_lock_unlock_flow_capability"
    },
    {
      "id": "blow_horn",
      "title": {
        "en": "Blow Horn"
      }
    },
    {
      "id": "flash_lights",
      "title": {
        "en": "Flash Lights"
      }
    },
    {
      "id": "send_message",
      "title": {
        "en": "Send Message"
      },
      "titleFormatted": {
        "en": "Send Message ([[subject]]) [[message]]"
      },
      "args": [
        {
          "type": "text",
          "name": "subject",
          "title": {
            "en": "Subject"
          },
          "placeholder": {
            "en": "Subject"
          }
        },
        {
          "type": "text",
          "name": "message",
          "title": {
            "en": "Message"
          },
          "placeholder": {
            "en": "Message"
          }
        }
      ]
    }
  ],
  "triggers": [
    {
      "id": "charging_status_change",
      "title": {
        "en": "Charging Status Changed"
      },
      "tokens": [
        {
          "name": "charging_status",
          "type": "string",
          "title": {
            "en": "Charging Status"
          },
          "example": "NOCHARGING"
        }
      ],
      "$filter": "capabilities=charging_status_capability"
    },
    {
      "id": "location_changed",
      "title": {
        "en": "Location Changed"
      },
      "tokens": [
        {
          "name": "Label",
          "type": "string",
          "title": {
            "en": "Label"
          },
          "example": "Home"
        },
        {
          "name": "Latitude",
          "type": "number",
          "title": {
            "en": "Latitude"
          },
          "example": "52.10017398097361"
        },
        {
          "name": "Longitude",
          "type": "number",
          "title": {
            "en": "Longitude"
          },
          "example": "5.044052884751771"
        },
        {
          "name": "Address",
          "type": "string",
          "title": {
            "en": "Address"
          },
          "example": "Vleutensebaan, 3544 Utrecht, Nederland"
        }
      ],
      "$filter": "capabilities=location_capability"
    },
    {
      "id": "drive_session_completed",
      "title": {
        "en": "Drive session completed"
      },
      "tokens": [
        {
          "name": "StartLabel",
          "type": "string",
          "title": {
            "en": "Starting Label"
          },
          "example": "Home"
        },
        {
          "name": "StartLatitude",
          "type": "number",
          "title": {
            "en": "Starting Latitude"
          },
          "example": "52.10017398097361"
        },
        {
          "name": "StartLongitude",
          "type": "number",
          "title": {
            "en": "Starting Longitude"
          },
          "example": "5.044052884751771"
        },
        {
          "name": "StartAddress",
          "type": "string",
          "title": {
            "en": "Starting Address"
          },
          "example": "Vleutensebaan, 3544 Utrecht, Nederland"
        },
        {
          "name": "StartMileage",
          "type": "number",
          "title": {
            "en": "Starting Mileage"
          },
          "example": "45875"
        },
        {
          "name": "EndLabel",
          "type": "string",
          "title": {
            "en": "End Label"
          },
          "example": "Home"
        },
        {
          "name": "EndLatitude",
          "type": "number",
          "title": {
            "en": "End Latitude"
          },
          "example": "52.10017398097361"
        },
        {
          "name": "EndLongitude",
          "type": "number",
          "title": {
            "en": "End Longitude"
          },
          "example": "5.044052884751771"
        },
        {
          "name": "EndAddress",
          "type": "string",
          "title": {
            "en": "End Address"
          },
          "example": "Vleutensebaan, 3544 Utrecht, Nederland"
        },
        {
          "name": "EndMileage",
          "type": "number",
          "title": {
            "en": "End Mileage"
          },
          "example": "45875"
        }
      ],
      "$filter": "capabilities=location_capability"
    },
    {
      "id": "refuelled",
      "title": {
        "en": "Refuelled"
      },
      "tokens": [
        {
          "name": "FuelBeforeRefuelling",
          "type": "number",
          "title": {
            "en": "Remaining fuel prior to refuelling"
          },
          "example": "9"
        },
        {
          "name": "FuelAfterRefuelling",
          "type": "number",
          "title": {
            "en": "Remaining fuel after refuelling"
          },
          "example": "46"
        },
        {
          "name": "RefuelledLiters",
          "type": "number",
          "title": {
            "en": "Amount refuelled"
          },
          "example": "37"
        },
        {
          "name": "Location",
          "type": "string",
          "title": {
            "en": "Location"
          },
          "example": "Proostwetering 90, 3543 AJ Utrecht"
        }
      ],
      "$filter": "capabilities=remanining_fuel_liters_capability"
    }
  ]
}