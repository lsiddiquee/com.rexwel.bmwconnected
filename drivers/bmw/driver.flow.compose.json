{
  "actions": [
    {
      "id": "climate_now",
      "title": {
        "en": "Climate Now"
      },
      "$filter": "capabilities=climate_now_capability"
    },
    {
      "id": "lock_vehicle",
      "title": {
        "en": "Lock"
      },
      "$filter": "capabilities=only_lock_unlock_flow_capability"
    },
    {
      "id": "unlock_vehicle",
      "title": {
        "en": "Unlock"
      },
      "$filter": "capabilities=only_lock_unlock_flow_capability"
    },
    {
      "id": "blow_horn",
      "title": {
        "en": "Blow Horn"
      }
    },
    {
      "id": "flash_lights",
      "title": {
        "en": "Flash Lights"
      }
    },
    {
      "id": "send_message",
      "title": {
        "en": "Send Message"
      },
      "titleFormatted": {
        "en": "Send Message ([[subject]]) [[message]]"
      },
      "args": [
        {
          "type": "text",
          "name": "subject",
          "title": {
            "en": "Subject"
          },
          "placeholder": {
            "en": "Subject"
          }
        },
        {
          "type": "text",
          "name": "message",
          "title": {
            "en": "Message"
          },
          "placeholder": {
            "en": "Message"
          }
        }
      ]
    }
  ],
  "triggers": [
    {
      "id": "charging_status_change",
      "title": {
        "en": "Charging Status Changed"
      },
      "tokens": [
        {
          "name": "charging_status",
          "type": "string",
          "title": {
            "en": "Charging Status"
          },
          "example": "NOCHARGING"
        }
      ],
      "$filter": "capabilities=charging_status_capability"
    },
    {
      "id": "location_changed",
      "title": {
        "en": "Location Changed"
      },
      "tokens": [
        {
          "name": "Label",
          "type": "string",
          "title": {
            "en": "Label"
          },
          "example": "Home"
        },
        {
          "name": "Latitude",
          "type": "number",
          "title": {
            "en": "Latitude"
          },
          "example": "52.10017398097361"
        },
        {
          "name": "Longitude",
          "type": "number",
          "title": {
            "en": "Longitude"
          },
          "example": "5.044052884751771"
        },
        {
          "name": "Address",
          "type": "string",
          "title": {
            "en": "Address"
          },
          "example": "Vleutensebaan, 3544 Utrecht, Nederland"
        }
      ]
    },
    {
      "id": "drive_session_completed",
      "title": {
        "en": "Drive session completed"
      },
      "tokens": [
