        },
        {
          "name": "StartAddress",
          "type": "string",
          "title": {
            "en": "Starting Address"
          },
          "example": "Vleutensebaan, 3544 Utrecht, Nederland"
        },
        {
          "name": "StartMileage",
          "type": "number",
          "title": {
            "en": "Starting Mileage"
          },
          "example": "45875"
        },
        {
          "name": "EndLabel",
          "type": "string",
          "title": {
            "en": "End Label"
          },
