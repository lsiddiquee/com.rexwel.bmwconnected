        },
        {
          "name": "EndLatitude",
          "type": "number",
          "title": {
            "en": "End Latitude"
          },
          "example": "52.10017398097361"
        },
        {
          "name": "EndLongitude",
          "type": "number",
          "title": {
            "en": "End Longitude"
          },
          "example": "5.044052884751771"
        },
        {
          "name": "EndAddress",
          "type": "string",
          "title": {
            "en": "End Address"
          },
          "example": "Vleutensebaan, 3544 Utrecht, Nederland"
        },
        {
          "name": "EndMileage",
          "type": "number",
          "title": {
            "en": "End Mileage"
          },
          "example": "45875"
        }
      ]
    },
    {
      "id": "refuelled",
      "title": {
        "en": "Refuelled"
      },
      "tokens": [
        {
          "name": "FuelBeforeRefuelling",
          "type": "number",
          "title": {
            "en": "Remaining fuel prior to refuelling"
          },
          "example": "9"
        },
        {
          "name": "FuelAfterRefuelling",
          "type": "number",
          "title": {
            "en": "Remaining fuel after refuelling"
          },
          "example": "46"
        },
        {
          "name": "RefuelledLiters",
          "type": "number",
          "title": {
            "en": "Amount refuelled"
          },
          "example": "37"
        },
        {
          "name": "Location",
          "type": "string",
          "title": {
            "en": "Location"
          },
          "example": "Proostwetering 90, 3543 AJ Utrecht"
        }
      ],
      "$filter": "capabilities=remaining_fuel_capability"
    },
    {
      "id": "geo_fence_enter",
